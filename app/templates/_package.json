{
  "name": "<%= projectName %>",
  "description": "<%= description %>",
  "version": "0.1.0",
  "author": "<%- projectAuthor %>",
  "scripts": {
    "clean": "npm install rimraf && rimraf dist node_modules typings",
    "build": "npm run tslint && npm run tsc && npm run license-check",
    "start": "npm run build && npm run serve",
    "start:dev": "npm run build && concurrently \"npm run tsc:w\" \"npm run serve:dev\" ",
    "aws": "node app.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "serve": "wsrv -O http://localhost:3000 -s -p 3000 -a 0.0.0.0",
    "serve:dev": "wsrv -O http://localhost:3000 -s -l -p 3000 -a 0.0.0.0",
    "tslint": "tslint -c tslint.json *.ts && tslint -c tslint.json 'app/{,**/}**.ts'",
    "link": "npm link alfresco-js-api ng2-activiti-analytics ng2-activiti-diagrams ng2-alfresco-core ng2-alfresco-datatable ng2-alfresco-documentlist ng2-alfresco-login ng2-alfresco-search ng2-alfresco-upload ng2-alfresco-viewer ng2-activiti-form ng2-activiti-tasklist ng2-alfresco-userinfo ng2-activiti-processlist ng2-alfresco-webscript ng2-alfresco-tag",
    "license-check": "echo 'license check is disabled'"
  },
  <% if (authorEmail) { %>
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= githubAccount %>/<%= projectName %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= githubAccount %>/<%= projectName %>/issues"
  },
  <% } %>
  "keywords": [
    "ng2",
    "angular",
    "angular2",
    "alfresco"
  ],
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "@types/node": "^6.0.42",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.23",

    "rimraf": "2.5.2",
    "material-design-icons": "2.2.3",
    "material-design-lite": "1.2.1",
    "ng2-translate": "2.5.0",
    "pdfjs-dist": "1.5.404",
    "flag-icon-css": "2.3.0",
    "intl": "1.2.4",
    "moment": "2.15.1",
    "chart.js": "^2.1.4",
    "ng2-charts": "1.1.0",
    "raphael": "^2.2.6",
    "md-date-time-picker": "^2.2.0",
    "dialog-polyfill": "^0.4.3",
    "element.scrollintoviewifneeded-polyfill": "^1.0.1",

    "alfresco-js-api": "^0.5.0",
    "ng2-activiti-analytics": "0.5.0",
    "ng2-activiti-diagrams": "0.5.0",
    "ng2-alfresco-core": "0.5.0",
    "ng2-alfresco-datatable": "0.5.0",
    "ng2-alfresco-documentlist": "0.5.0",
    "ng2-alfresco-login": "0.5.0",
    "ng2-alfresco-search": "0.5.0",
    "ng2-alfresco-upload": "0.5.0",
    "ng2-alfresco-viewer": "0.5.0",
    "ng2-activiti-form": "0.5.0",
    "ng2-activiti-tasklist": "0.5.0",
    "ng2-alfresco-userinfo": "0.5.0",
    "ng2-activiti-processlist": "0.5.0",
    "ng2-alfresco-webscript": "0.5.0",
    "ng2-alfresco-tag": "0.5.0"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.32",
    "@types/jasmine": "^2.2.33",
    "concurrently": "^2.2.0",
    "license-check": "1.1.5",
    "mime": "^1.3.4",
    "tslint": "3.15.1",
    "typescript": "^2.0.3",
    "wsrv": "^0.1.5"
  }
}
